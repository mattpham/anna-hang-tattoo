{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": ["Link"],
        "specialLink": ["to"],
        "aspects": ["noHref", "invalidHref", "preferButton"]
      }
    ],
    "import/no-extraneous-dependencies": "off",
    "react/forbid-prop-types": "off"
  }
}
